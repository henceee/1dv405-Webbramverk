<% if flash[:notice] %>
    <div class="notice">
        <%= flash[:notice] %>
    </div>
<% end %>
<h2>Admin</h2>
<ul>
<% @users.each do |user| %>
  <li><h4><%= user.name %></h4>
  
        <ul>
        <% user.applications.each do |app|%>
        <li> <%= link_to(app.name, showapp_path(app)) %> <%= link_to("Revoke", destroyapp_path(app)) %></li>
        <% end %>
        </ul>
  </li>
<% end %>
</ul>

<p><%= link_to("Log  Out", logout_path) %></p>